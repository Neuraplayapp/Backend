[32m+[m[32m// Debug Render deployment locally with exact same environment[m
[32m+[m[32m// Run with: node debug-render-local.js[m
[32m+[m
[32m+[m[32m// Set exact Render environment variables[m
[32m+[m[32mprocess.env.NODE_ENV = 'production';[m
[32m+[m[32mprocess.env.PORT = process.env.PORT || '3000';[m
[32m+[m[32mprocess.env.RENDER = 'true';[m
[32m+[m
[32m+[m[32m// Load production env if it exists[m
[32m+[m[32mtry {[m
[32m+[m[32m  const fs = require('fs');[m
[32m+[m[32m  if (fs.existsSync('production.env')) {[m
[32m+[m[32m    require('dotenv').config({ path: 'production.env' });[m
[32m+[m[32m    console.log('‚úÖ Loaded production.env');[m
[32m+[m[32m  }[m
[32m+[m[32m} catch (error) {[m
[32m+[m[32m  console.log('‚ö†Ô∏è No production.env found, using system env');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Simulate Render paths[m
[32m+[m[32mprocess.env.RENDER_EXTERNAL_URL = process.env.RENDER_EXTERNAL_URL || 'http://localhost:3000';[m
[32m+[m
[32m+[m[32mconsole.log('üöÄ Starting Render simulation...');[m
[32m+[m[32mconsole.log('üìç Environment:', process.env.NODE_ENV);[m
[32m+[m[32mconsole.log('üìç Port:', process.env.PORT);[m
[32m+[m[32mconsole.log('üìç Render mode:', process.env.RENDER);[m
[32m+[m
[32m+[m[32m// Start the exact same way Render does[m
[32m+[m[32mtry {[m
[32m+[m[32m  require('./server-new.cjs');[m
[32m+[m[32m} catch (error) {[m
[32m+[m[32m  console.error('üí• Render simulation failed:', error);[m
[32m+[m[32m  console.error('Stack trace:', error.stack);[m
[32m+[m[32m}[m
